<h1>Carrito de Compras</h1>

<div class="carrito">
    {{#each productos}}
        <div class="producto-carrito">
            <h2>{{this.product.title}}</h2>
            <p>Precio: ${{this.product.price}}</p>
            <p>Cantidad: {{this.quantity}}</p>
            <p>Subtotal: ${{multiply this.product.price this.quantity}}</p>
        </div>
    {{/each}}
</div>